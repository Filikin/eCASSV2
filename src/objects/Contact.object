    <validationRules>
        <fullName>Client_Homeless</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(X7b_Livin_Where__c) = &quot;3. Homeless&quot;,
TEXT(X7a_Living_with_whom__c) &lt;&gt; &quot;7. Other&quot;)</errorConditionFormula>
        <errorMessage>If the client is homeless or in an institution, please, select Q7a = &quot;7. Other&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Employment_status_other_specified</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT( X11_Employement_Status__c ) = &quot;8. Other (specify)&quot; , ISBLANK( X11_Employement_Status_other__c ))</errorConditionFormula>
        <errorDisplayField>X11_Employement_Status_other__c</errorDisplayField>
        <errorMessage>If Employment status = 8. Other (specify), please complete Q11 Employment Status other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Nationality_Other_Specified</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT( X10a_Nationality__c ) = &quot;2. Other&quot;,  ISBLANK( X10a_Nationality_Other__c ))</errorConditionFormula>
        <errorDisplayField>X10a_Nationality_Other__c</errorDisplayField>
        <errorMessage>If 2, please complete 10a Nationality Other</errorMessage>
    </validationRules>
